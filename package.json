{
  "name": "cendas",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:production": "webpack --config webpack.config.js && serve -s build",
    "start": "webpack serve",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "prebuild": "rimraf ./build",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watchAll"
